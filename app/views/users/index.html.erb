<div class="container">
<div class="row">
<div class="col-md-3">

<%= render 'books/x', user: current_user %>
<%= render 'books/w', book: @book %>

</div>


<div class="col-md-8 offset-md-1">
<h1>Users</h1>
  <table class="table table-striped">

      <thead>
        <tr>
          <th>Image</th>
          <th>Name</th>
          <th></th>
        </tr>
      </thead>

      <tbody>
       <% @users.each do |user| %>
        <tr>
          <td><%=link_to user_path(user.id) do%>
            <%= image_tag user.get_profile_image %>
            <%end%></td>
          <td><%= user.name %></td>
          <td><%=link_to("Show", user_path(user.id))%></td>
        </tr>
       <% end %>
      </tbody>

   </table>
</div>
</div>
</div>