<h1>共通レイアウト</h1>

<h1>Users</h1>



<table>

      <thead>
        <tr>
          <th>Image</th>
          <th>Name</th>
          <th></th>
        </tr>
      </thead>

      <tbody>
       <% @users.each do |user| %>
        <tr>
          <td><%= image_tag user.get_image %></td>
          <td><%= user.name %></td>
          <td><%=link_to("Show", user_path(user.id))%></td>

        </tr>
       <% end %>
      </tbody>

    </table>
